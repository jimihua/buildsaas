<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="package-tenant" extends="json-default"
		namespace="/">
		<action name="tenant_*" method="{1}"
			class="com.buildsaas.action.TenantAction">
			<result name="regSuccess" type="json">
				<param name="root">hashMap</param>
			</result>
			<result name="loginSuccess" type="json">
				<param name="root">hashMap</param>
			</result>
			<result name="isRegSuccess" type="json">
				<param name="root">hashMap</param>
			</result>
		</action>
	</package>
</struts>
